<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="80dp">

    <ImageView
        android:id="@+id/activity_mypage_setup_imageview"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="6dp"
        android:layout_marginBottom="6dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_activity_mypage_profile_act" />

    <TextView
        android:id="@+id/activity_mypage_setup_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/activity_mypage_setup_imageview"
        app:layout_constraintStart_toEndOf="@+id/activity_mypage_setup_imageview"
        app:layout_constraintTop_toTopOf="@+id/activity_mypage_setup_imageview"
        tools:text="내용" />

    <ImageView
        android:id="@+id/activity_mypage_setup_next"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginEnd="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/activity_mypage_setup_imageview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/activity_mypage_setup_imageview"
        app:srcCompat="@drawable/ic_activity_mypage_profile_next" />

</androidx.constraintlayout.widget.ConstraintLayout>