<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".VibeDetailActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolBar_top"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/tv_logo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="5dp"
            android:text="#설레는"
            android:textColor="@color/black"
            android:textSize="15dp"
            android:textStyle="bold" />

    </androidx.appcompat.widget.Toolbar>

    <ScrollView
        android:id="@+id/scrollView_main"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@+id/toolBar_top"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            app:layout_constraintTop_toBottomOf="@+id/toolBar_top"
            android:orientation="vertical">

            <TableLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:layout_gravity="center"
                android:stretchColumns="*">

                <TableRow
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="10dp">

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/imageViewMain1"
                        android:layout_width="50dp"
                        android:layout_height="200dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/back_txt" />

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/imageViewMain2"
                        android:layout_width="50dp"
                        android:layout_height="200dp"
                        android:background="@color/back_txt"/>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="10dp">

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/imageViewMain3"
                        android:layout_width="50dp"
                        android:layout_height="200dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/back_txt" />

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/imageViewMain4"
                        android:layout_width="50dp"
                        android:layout_height="200dp"
                        android:background="@color/back_txt" />
                </TableRow>
                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="10dp">

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/imageViewMain5"
                        android:layout_width="50dp"
                        android:layout_height="200dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/back_txt" />

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/imageViewMain6"
                        android:layout_width="50dp"
                        android:layout_height="200dp"
                        android:background="@color/back_txt" />
                </TableRow>
                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="10dp">

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/imageViewMain7"
                        android:layout_width="50dp"
                        android:layout_height="200dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/back_txt" />

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/imageViewMain8"
                        android:layout_width="50dp"
                        android:layout_height="200dp"
                        android:background="@color/back_txt" />
                </TableRow>
                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="10dp">

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/imageViewMain9"
                        android:layout_width="50dp"
                        android:layout_height="200dp"
                        android:layout_marginRight="10dp"
                        android:background="@color/back_txt" />

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/imageViewMain10"
                        android:layout_width="50dp"
                        android:layout_height="200dp"
                        android:background="@color/back_txt" />
                </TableRow>
            </TableLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btn_addpostdetail"
                android:layout_width="52dp"
                android:layout_height="52dp"
                android:layout_gravity="right"
                android:layout_marginRight="10dp"
                android:background="@drawable/ic_activity_vibe_main_postaddbtn" />
        </LinearLayout>
    </ScrollView>



</androidx.constraintlayout.widget.ConstraintLayout>