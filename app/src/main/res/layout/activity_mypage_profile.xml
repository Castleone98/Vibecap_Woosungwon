<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="80dp">

    <ImageView
        android:id="@+id/activity_mypage_alarm_imageview"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_activity_mypage_profile_act" />

    <TextView
        android:id="@+id/activity_mypage_alarm_title"
        android:layout_width="200dp"
        android:layout_height="20dp"
        android:layout_marginStart="16dp"
        android:textSize="14sp"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/activity_mypage_alarm_imageview"
        app:layout_constraintStart_toEndOf="@+id/activity_mypage_alarm_imageview"
        app:layout_constraintTop_toTopOf="@+id/activity_mypage_alarm_imageview"
        tools:text="내용" />

    <ImageView
        android:id="@+id/activity_mypage_profile_next"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginEnd="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/activity_mypage_alarm_imageview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/activity_mypage_alarm_imageview"
        app:srcCompat="@drawable/ic_activity_mypage_profile_next" />

</androidx.constraintlayout.widget.ConstraintLayout>